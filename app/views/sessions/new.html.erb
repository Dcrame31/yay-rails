<h1>Log In</h1>
   <%= form_tag('/login', method: "post") do %>
   <%= label_tag :name %>
   <%= collection_select(:user, :username, User.all, :username, :username, prompt: 'Please select your username from this dropdown') %><br>
   <%= label_tag :password%>
   <%= password_field_tag :password%><br>
   <%= submit_tag "Sign In"%>
<%end%>